<div class='row'>
  <div class='col-12 col-md-6 col-lg-4'>
    <div class='card bg-white'>
      <h1 class='ml-3 my-2'>{{moment-format model.date 'MMM D'}}</h1>
      <table class='table mb-0'>
        <tbody>
          {{#each model.dayPlan as |passage|}}
            <tr>
              <td>{{passage}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
